@Library('pipeline-library')

def deploy_env= 'ABC'

pipeline {
    agent any

    stages {

       stage('List Values') {
            steps {
               script {
                def App_Name = 'Varun'
                def list_val = lists.listvalue(App_Name)
                echo "Values: ${list_val}"
                }
            }
        }
      
    }
}
