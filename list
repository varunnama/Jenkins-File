@Library('pipeline-library')

def deploy_env= 'ABC'

pipeline {
    agent any

    stages {

       stage('List Values') {
            steps {
               script {
                def App_Name = 'Varun'
                def list_val = lists.listvalue(App_Name)
                when{
		expression {list_val.size() >= 1}
		echo "Values: ${list_val}"
		    }
                
                }
            }
        }
      
    }
}
